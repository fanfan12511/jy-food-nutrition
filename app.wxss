.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* 食物列表 */
.result_item{
  overflow: hidden;
  padding: 20rpx;
  border-bottom: 1px solid #ccc;
}
.item_img{
  float: left;
  padding:0 20rpx;
}
.item_img image{
  display: block;
  width: 100rpx;
  height: 100rpx;
}
.item_info{
  float: left
}
.info_name{
  font-size: 30rpx;
  padding: 10rpx 0;
}
.info_cont{
  font-size: 25rpx;
  color:#777;
}
.info_cont_num{
  float: left;
  color:#eb6100;
  padding-top: 5rpx;
}
.info_cont_unit{
  float: left;
  padding: 5rpx;
}

/** 头部搜索 **/
.index-top{
  position: relative;
}
.index-top-bg{
  width: 100%;
  height: 350rpx;
}
.index-top-search{
  position: absolute;
  left: 50%;
  bottom: 20%;
  transform: translateX(-50%);
  width: 80%;
  background: #fff;
  overflow: hidden
}
.search-icon{
  float: left;
  padding: 30rpx 30rpx;
}
.search-input{
  padding: 18rpx 0;
  font-size: 30rpx;
}
/*  排名，食谱 列表 */
.index-list{
  overflow: hidden;
}
.index-list-item{
  border: 3rpx solid #ccc;
  padding: 10rpx 20rpx;
  border-radius: 10rpx;
  margin-top: 10rpx;
}
.item-img{
  float: left;
  width: 70rpx;
  height: 70rpx;
}
.item-text{
  height: 70rpx;
  line-height:70rpx;
  padding-left: 20rpx;
  color:#666;
}