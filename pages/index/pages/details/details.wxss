.result_list{
  overflow: hidden;
}
.result_item{
  overflow: hidden;
  padding: 20rpx;
  margin: 20rpx 30rpx;
  background: #eb6100;
  border-radius: 20rpx;
  color:#fff;
}
.item_img{
  float: left;
  padding:0 20rpx;
}
.item_img image{
  display: block;
  width: 150rpx;
  height: 150rpx;
}
.item_info{
  float: left
}
.info_name{
  font-size: 35rpx;
  padding: 10rpx 0 20rpx 0;
}
.info_cont{
  font-size: 35rpx;
}
.info_cont_num{
  float: left;
}
.canvas_box{
  width: 28%;
  float: left;
  margin: 10rpx 2.5%;
}
.canvas_main{
  overflow: hidden;
  margin-left: -20rpx;
}
.more_nutrition{
  margin: 20rpx auto;
  text-align: center;
}
.foot_nutrition{
  position: fixed;
  bottom: 0;
  width: 100%;
  overflow: hidden;
}
.foot_btn{
  float: left;
  width: 49.8%;
  height: 50rpx;
  background: #ccc;
  text-align: center;
  padding: 20rpx 0;
  box-sizing: none
}
.foot_btn_right{
  float: right;
}